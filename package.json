    "prepublishOnly": "npm run check && npm run build-svelte"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.4",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.5.4",
    "@types/uuid": "^10.0.0",
    "svelte": "^4.0.0",
    "svelte-check": "^3.8.5",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.2.1"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "dependencies": {
    "uuid": "^10.0.0"
  }
