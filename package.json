{
  "name": "sly-svelte-file-tree",
  "private": false,
  "version": "1.0.1",
  "type": "module",
  "description": "A customizable file tree view component for Svelte",
  "keywords": [
    "svelte",
    "slyotis",
    "sly",
    "file-tree",
    "tree-view",
    "file",
    "tree",
    "folder-tree",
    "folder-view",
    "file-select"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SlyOtis/svelte-file-tree"
  },
  "homepage": "https://github.com/SlyOtis/svelte-file-tree",
  "bugs": {
    "url": "https://github.com/SlyOtis/svelte-file-tree/issues"
  },
  "author": "Martin Ottersland",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-svelte": "svelte-package",
    "preview": "vite preview",
